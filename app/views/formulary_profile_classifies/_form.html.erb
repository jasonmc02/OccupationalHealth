<%= form_for @formulary_profile_classify, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :formulary_profile_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :formulary_profile_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :project, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :project, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :part_program, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :part_program, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :entire_program, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :entire_program, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :other, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :other, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :which, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :which, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                formulary_profile_classifies_path, :class => 'btn' %>
  </div>
<% end %>
