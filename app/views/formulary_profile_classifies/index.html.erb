<%- model_class = FormularyProfileClassify -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:formulary_profile_id) %></th>
      <th><%= model_class.human_attribute_name(:project) %></th>
      <th><%= model_class.human_attribute_name(:part_program) %></th>
      <th><%= model_class.human_attribute_name(:entire_program) %></th>
      <th><%= model_class.human_attribute_name(:other) %></th>
      <th><%= model_class.human_attribute_name(:which) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @formulary_profile_classifies.each do |formulary_profile_classify| %>
      <tr>
        <td><%= link_to formulary_profile_classify.id, formulary_profile_classify_path(formulary_profile_classify) %></td>
        <td><%= formulary_profile_classify.formulary_profile_id %></td>
        <td><%= formulary_profile_classify.project %></td>
        <td><%= formulary_profile_classify.part_program %></td>
        <td><%= formulary_profile_classify.entire_program %></td>
        <td><%= formulary_profile_classify.other %></td>
        <td><%= formulary_profile_classify.which %></td>
        <td><%=l formulary_profile_classify.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_formulary_profile_classify_path(formulary_profile_classify), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      formulary_profile_classify_path(formulary_profile_classify),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_formulary_profile_classify_path,
            :class => 'btn btn-primary' %>
