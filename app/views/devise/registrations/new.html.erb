<%= devise_error_messages! %>
<div id="sign_up1">
  <div class="container">
    <div class="row">
      <div class="col-md-12 header">
        <h4><%= t "helpers.devise.sign_up_page.text_1" %></h4>
        <p><%= t "helpers.devise.sign_up_page.text_2" %></p>
      </div>
      <div class="col-sm-3 division"></div>
      <div class="col-md-12 footer">
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { :class => "form-inline" } ) do |f| %>
          <%= f.email_field :email, autofocus: true, :class => "form-control", :placeholder => t("helpers.devise.sign_up_page.text_4") %>
          <%= f.password_field :password, autocomplete: "off", :class => "form-control", :placeholder => t("helpers.devise.sign_up_page.text_5") %>
          <%= f.password_field :password_confirmation, autocomplete: "off", :class => "form-control", :placeholder => t("helpers.devise.sign_up_page.text_6") %>
          <%= f.submit t("helpers.devise.sign_up_page.text_7") %>
        <% end %>
      </div>
      <div class="col-md-12 dosnt">
        <span><%= t "helpers.devise.sign_up_page.text_8" %></span>
        <a href="<%= new_user_session_path %>"><%= t "helpers.devise.sign_up_page.text_9" %></a>
      </div>
    </div>
  </div>
</div>