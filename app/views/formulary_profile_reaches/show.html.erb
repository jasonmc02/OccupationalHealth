<%- model_class = FormularyProfileReach -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:research) %>:</strong></dt>
  <dd><%= @formulary_profile_reach.research %></dd>
  <dt><strong><%= model_class.human_attribute_name(:action) %>:</strong></dt>
  <dd><%= @formulary_profile_reach.action %></dd>
  <dt><strong><%= model_class.human_attribute_name(:policy) %>:</strong></dt>
  <dd><%= @formulary_profile_reach.policy %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              formulary_profile_reaches_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_formulary_profile_reach_path(@formulary_profile_reach), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              formulary_profile_reach_path(@formulary_profile_reach),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
