<%- model_class = FormularyProfileReach -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:research) %></th>
      <th><%= model_class.human_attribute_name(:action) %></th>
      <th><%= model_class.human_attribute_name(:policy) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @formulary_profile_reaches.each do |formulary_profile_reach| %>
      <tr>
        <td><%= link_to formulary_profile_reach.id, formulary_profile_reach_path(formulary_profile_reach) %></td>
        <td><%= formulary_profile_reach.research %></td>
        <td><%= formulary_profile_reach.action %></td>
        <td><%= formulary_profile_reach.policy %></td>
        <td><%=l formulary_profile_reach.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_formulary_profile_reach_path(formulary_profile_reach), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      formulary_profile_reach_path(formulary_profile_reach),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_formulary_profile_reach_path,
            :class => 'btn btn-primary' %>
